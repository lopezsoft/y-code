{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "cossou/jasperphp": "^2.8",
        "doctrine/dbal": "^3.6.2",
        "dompdf/dompdf": "^2.0",
        "free2one/php-accessor": "~0.3",
        "guzzlehttp/guzzle": "^7.5.0",
        "laravel/framework": "^v10.9.0",
        "laravel/passport": "^v11.8.4",
        "laravel/tinker": "^v2.8.1",
        "laraveles/spanish": "^1.5",
        "league/flysystem-aws-s3-v3": "3.0",
        "maatwebsite/excel": "^3.1.48",
        "phpoffice/phpspreadsheet": "^1.28.0"
    },
    "require-dev": {
        "fakerphp/faker": "^v1.21.0",
        "laravel/breeze": "^v1.20.2",
        "laravel/sail": "^v1.21.5",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^v7.5.2",
        "phpunit/phpunit": "^10.1.2",
        "spatie/laravel-ignition": "^2.1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
