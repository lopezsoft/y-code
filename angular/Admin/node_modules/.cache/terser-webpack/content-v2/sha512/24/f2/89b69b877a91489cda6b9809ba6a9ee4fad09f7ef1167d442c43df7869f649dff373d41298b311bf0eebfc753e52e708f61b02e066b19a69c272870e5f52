t_id:[\"\"],unit_name:[\"\"],factor:[0],purchase_cost:[0],sale_price:[0]})}get invalidName(){return this.customForm.get(\"term_name\").invalid&&this.customForm.get(\"term_name\").touched}get invalidAttributeId(){return 0===this.customForm.get(\"attribute_id\").value}ngOnInit(){super.ngOnInit();const ts=this;ts.PutURL=\"/products/items/update/\",ts.PostURL=\"/products/items/create\",ts.showSpinner(),ts.attrSer.getAll({}).subscribe(resp=>{ts.attributes=resp}),ts.classSer.getData().subscribe(resp=>{ts.classof=resp}),ts.brandSer.getAll().subscribe(resp=>{ts.brands=resp}),ts.cateSer.getParents({}).subscribe(resp=>{ts.categories=resp}),ts.taxSer.getData({isVat:1}).subscribe(resp=>{ts.taxBill=resp,ts.taxSale=resp,resp.length>0&&!ts.editing&&(ts.customForm.get(\"tax_bill_id\").setValue(resp[0].id),ts.customForm.get(\"tax_sales_id\").setValue(resp[0].id))}),ts.measSer.getAll({}).subscribe(resp=>{ts.mUnits=resp,ts.hideSpinner()}),ts.measSer.getBase().subscribe(resp=>{ts.units=resp})}saveData(){const ts=this,frm=ts.customForm,lang=ts.translate;let values={};if(!frm.invalid){ts.showSpinner(),values=frm.value,ts.changeImage&&(values.imgdata=ts.imgData,values.imgname=ts.imgname);const data={records:JSON.stringify(values),units:JSON.stringify(ts.measurementUnits)};ts.editing?ts.api.put(`${ts.PutURL}${ts.uid}`,data).subscribe(resp=>{ts.msg.toastMessage(lang.instant(\"general.savedSuccessfully\"),resp.message,0),ts.editing=!1,ts.toClose?ts.close():(ts.onResetForm(),ts.focusElement.nativeElement.focus()),ts.hideSpinner(),ts.disabledLoading()},err=>{ts.msg.toastMessage(lang.instant(\"general.error\"),err.error.message,4),ts.disabledLoading(),ts.hideSpinner()}):ts.api.post(ts.PostURL,data).subscribe(resp=>{ts.msg.toastMessage(lang.instant(\"general.successfullyCreated\"),resp.message,0),ts.toClose?ts.close():(ts.onResetForm(),ts.focusElement.nativeElement.focus()),ts.disabledLoading(),ts.hideSpinner()},err=>{ts.msg.toastMessage(lang.instant(\"general.error\"),err.error.message,4),ts.di